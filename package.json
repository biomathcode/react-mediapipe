{
  "name": "react-mediapipe",
  "version": "0.0.1",
  "description": "A starter for creating a React component react-mediapipe.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/biomathcode/react-mediapipe#readme",
  "bugs": {
    "url": "https://github.com/biomathcode/react-mediapipe/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/biomathcode/react-mediapipe.git"
  },
  "author": "Pratik Sharma <sharma.pratik2016@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  },
  "keywords": [],
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "dev": "concurrently \"pnpm build --watch\" \"pnpm storybook\" \"pnpm test\" ",
    "build": "tsdown",
    "play": "vite --config playground/vite.config.ts",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --write --unsafe .",
    "lint:ci": "biome check --unsafe .",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "commit": "cz",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "release": "pnpm build && pnpm release-it",
    "link:self": "pnpm link --global",
    "prepare": "lefthook install",
    "deploy-storybook": "gh-pages -d storybook-static",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-test-renderer": "^19.1.0",
    "@vitest/browser-playwright": "^4.0.12",
    "@vitest/coverage-v8": "^4.0.12",
    "concurrently": "^9.2.1",
    "gh-pages": "^6.3.0",
    "playwright": "^1.56.1",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "release-it": "^19.0.6",
    "storybook": "^10.0.8",
    "tsdown": "^0.16.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "dependencies": {
    "@mediapipe/tasks-vision": "0.10.22-rc.20250304"
  }
}